{
  "name": "Mappl",
  "version": "1.0.0",
  "description": "Location-based event discovery platform",
  "runtime": "nodejs-20.0",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "env": [
    "NEXT_PUBLIC_APPWRITE_ENDPOINT",
    "NEXT_PUBLIC_APPWRITE_PROJECT_ID",
    "NEXT_PUBLIC_APPWRITE_API_KEY",
    "NEXT_PUBLIC_APPWRITE_DATABASE_ID",
    "NEXT_PUBLIC_APPWRITE_EVENTS_COLLECTION_ID",
    "NEXT_PUBLIC_APPWRITE_USERS_COLLECTION_ID",
    "NEXT_PUBLIC_APPWRITE_MESSAGES_COLLECTION_ID",
    "NEXT_PUBLIC_APPWRITE_BUCKET_ID",
    "NEXT_PUBLIC_APPWRITE_ADMIN_TEAM_ID",
    "NEXT_PUBLIC_APPWRITE_SITE_URL",
    "GOOGLE_SITE_VERIFICATION"
  ]
}
